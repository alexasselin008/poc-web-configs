{
  "name": "@alexasselin/eslint-plugin",
  "author": "Groupe Sharegate inc.",
  "description": "Sharegates ESLint rules and configs.",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "keywords": [
    "sharegate",
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexasselin008/poc-web-configs.git",
    "directory": "packages/eslint-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "module": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup lib",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@next/eslint-plugin-next": "*",
    "eslint-plugin-sort-imports-es6-autofix": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "eslint-plugin-lodash": "*",
    "requireindex": "1.2.0"
  },
  "devDependencies": {
    "typescript": "4.9.4",
    "@types/eslint": "8.4.10",
    "@types/requireindex": "1.2.0",
    "@alexasselin/typescript-config": "*"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8"
  }
}
