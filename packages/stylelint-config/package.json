{
    "name": "@alexasselin/stylelint-config",
    "author": "Groupe Sharegate inc.",
    "version": "0.0.0",
    "description": "Sharegate recommended stylelint config.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alexasselin008/poc-web-configs.git",
        "directory": "packages/stylelint-config"
    },
    "license": "Apache-2.0",
    "keywords": [
        "sharegate",
        "stylelint",
        "stylelint-plugin",
        "config",
        "css"
    ],
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "module": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup index.ts",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
    },
    "peerDependencies": {
        "stylelint": ">=14.10.0"
    },
    "dependencies": {
        "stylelint-config-recommended": "^9",
        "@alexasselin/typescript-config": "*"
    }
}
