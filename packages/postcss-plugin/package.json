{
    "name": "@alexasselin/postcss-plugin",
    "author": "Groupe Sharegate inc.",
    "version": "0.0.0",
    "description": "Sharegate recommended typescript config.",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alexasselin008/poc-web-configs.git",
        "directory": "packages/postcss-plugin"
    },
    "license": "Apache-2.0",
