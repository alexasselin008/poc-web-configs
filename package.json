{
  "name": "poc-web-configs",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "typecheck": "cd ./packages/browserslist-config && npm run typecheck",
    "build": "cd ./packages/browserslist-config && npm run build",
    "clean": "cd ./packages/browserslist-config && npm run clean",
    "lint:eslint": "eslint ./packages/**/*.js",
    "test": "jest",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@alexasselin/eslint-config": "*",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.0",
    "jest": "29.3.1",
    "rimraf": "3.0.2",
    "tsup": "6.5.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
